<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0 //EN" 
                             "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.spring.biz.board.BoardVo"  alias="BoardVo" />
		<typeAlias type="com.spring.biz.login.LoginVo"  alias="LoginVo" />
		<typeAlias type="com.spring.biz.review.ReviewVo"  alias="ReviewVo" />
		<typeAlias type="com.spring.biz.cart.CartVo"  alias="CartVo" />
    </typeAliases>
	
	<mappers>
		<mapper resource="mappings/board-mapping.xml" />
		<mapper resource="mappings/login-mapping.xml" />
		<mapper resource="mappings/review-mapping.xml" />
		<mapper resource="mappings/cart-mapping.xml" />
	</mappers>	

</configuration>